pipeline {
    agent any
        stages{
            stage('test'){
                steps{
                    echo 'testing this'
                }
            }
            stage('build'){
                steps{
                    echo 'building this'
                    sh '''
                        ls -l
                        pwd 
                    '''
                }
            }
            stage('deploy'){
                steps{
                    echo 'deploying this'
                }
            }
        }
}